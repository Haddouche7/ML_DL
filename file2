# Importer les librairies
import numpy as np
import pandas as pd
import matplotlib.pyplot as plt

# Creer un dataset
df = pd.DataFrame(
        {"velo":[23, 45, 70, 55, 23],
         "bus":[34, 67, 88, 10, 20],
         "uber":[22, 33, 44, 55, 66],
         "metro":[10, 40, 27, 88, 54]
                })

# Moyenne
df.mean()

# Médiane
df.median()

# Mode
df.mode()

# ecart type
df.std()

# Moyenne Vélo
moyenne_velo = df["velo"].mean()

count = df["velo"].count()

ecart_type = df["uber"].std()



print("Voici le premier quartile")
df.quantile(0.25)


df.plot(kind = "box")
plt.savefig("boxplot.png")
m

df.to_csv("dataframe.csv")
